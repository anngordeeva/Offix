{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": false,
  "printWidth": 110,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["^.*/.*.scss$", "", "<THIRD_PARTY_MODULES>", "", "^~/(.*)$", "", "^[./]"]
}
